<% include ./header %>

<div class="container">
    <div class="center-template">    
      <h1>Détail des assureurs par modèle de voiture</h1>
      <table>
        <thead>
          <th>Code du modèle</th>
          <th>Libellé</th>
          <th>Numéro de contrat</th>
          <th>Type de contrat</th>
          <th>Nom de l'assureur</th>
          <th>Adresse de l'assureur</th>
          <th>Téléphone de l'assureur</th>
        </thead>
        <tbody>
          <%_ data.forEach(function(el){_%>
            <tr>
              <td><%=el.Code %></td>
              <td><%=el.Libelle %></td>
              <td><%=el.NumContrat %></td>
              <td><%=el.Type %></td>
              <td><%=el.NomAssureur %></td>
              <td><%=el.AdresseAssureur %></td>
              <td><%=el.Telephone %></td>
            </tr>
          <%_ }); _%>
        </tbody>
      </table>
      <hr>
  </div>
</div>
<% include ./footer %>