<% include ./header %>

<div class="container">
    <div class="center-template">    
      <h1>Table <%= table %></h1>
      <table>
        <thead>
          <th>Numéro de réservation</th>
          <th>Numéro de client</th>
          <th>Etat</th>
          <th>Date</th>
          <th>Voiture concernée</th>
          <th>Type de location</th>
          <th>Date d'annulation</th>
          <th>Référence de nouvelle réservation</th>
        </thead>
        <tbody>
          <%_ data.forEach(function(el){_%>
            <tr>
              <td><%=el.ID %></td>
              <td><%=el.NClient %></td>
              <td><%=el.Etat %></td>
              <td><%=el.Date %></td>
              <td><%=el.Voiture %></td>
              <td><%=el.TypeLoc %></td>
              <td><%=el.DateSuppression %></td>
              <td><%=el.NouvelleReserv %></td>
            </tr>
          <%_ }); _%>
        </tbody>
      </table>
      <hr>
  </div>
</div>
<% include ./footer %>