<% include ./header %>

<div class="container">
    <div class="center-template">    
      <h1>Table <%= table %></h1>
      <table>
        <thead>
          <th></th>
          <th>Nom</th>
          <th>Adresse</th>
          <th>Telephone</th>
          <th>Fax</th>
        </thead>
        <tbody>
          <%_ data.forEach(function(el){_%>
            <tr>
              <td><a href='supprimerAssureur/<%=el.Nom%>,<%=el.Adresse%>' title="Supprimer cette ligne"><img src="/images/supp.png" alt="suppression"></a>
                <a href='editerAssureur/<%=el.Nom%>,<%=el.Adresse%>' title="Ã‰diter cette ligne"><img src="/images/edit.png" alt="edition"></a></td>
              <td><%=el.Nom %></td>
              <td><%=el.Adresse %></td>
              <td><%=el.Telephone %></td>
              <td><%=el.Fax %></td>
            </tr>
          <%_ }); _%>
        </tbody>
      </table>
      <hr>
  </div>
</div>
<% include ./footer %>