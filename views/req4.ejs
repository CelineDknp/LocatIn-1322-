<% include ./header %>

<div class="container">
    <div class="center-template">    
      <h1>Transactions hebdomadaires</h1>
      <table>
        <thead>
          <th>Libellé</th>
          <th>Code du modèle</th>
          <th>Nombre de voitures</th>
          <th>Prix moyen</th>
        </thead>
        <tbody>
          <%_ data.forEach(function(el){_%>
            <tr>
              <td><%=el.Libelle %></td>
              <td><%=el.Code %></td>
              <td><%=el.Voitures %></td>
              <td><%=Math.round(el.Prix) %>€</td>
            </tr>
          <%_ }); _%>
        </tbody>
      </table>
      <hr>
  </div>
</div>
<% include ./footer %>