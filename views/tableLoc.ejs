<% include ./header %>

<div class="container">
    <div class="center-template">    
      <h1>Table <%= table %></h1>
      <table>
        <thead>
          <th></th>
          <th>Numéro de réservation</th>
          <th>Numéro de permis du client</th>
          <th>Kilométrage au départ</th>
          <th>Caution</th>
        </thead>
        <tbody>
          <%_ data.forEach(function(el){_%>
            <tr>
              <td><a href='supprimerModele/<%=el.Nom%>,<%=el.Adresse%>' title="Supprimer cette ligne"><img src="/images/supp.png" alt="suppression"></a>
                <a href='editerModele/<%=el.Nom%>,<%=el.Adresse%>' title="Éditer cette ligne"><img src="/images/edit.png" alt="edition"></a></td>
              <td><%=el.NReserv %></td>
              <td><%=el.NPermis %></td>
              <td><%=el.KMDepart %></td>
              <td><%=el.Caution %></td>
            </tr>
          <%_ }); _%>
        </tbody>
      </table>
      <hr>
  </div>
</div>
<% include ./footer %>