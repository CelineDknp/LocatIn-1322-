<% include ./header %>

<div class="container">
    <div class="center-template">    
      <h1>Table <%= table %></h1>
      <table>
        <thead>
          <th></th>
          <th>Code</th>
          <th>Libellé</th>
          <th>Marque</th>
          <th>Type</th>
          <th>Puissance</th>
          <th>Classe de tarification</th>
        </thead>
        <tbody>
          <%_ data.forEach(function(el){_%>
            <tr>
              <td><a href='supprimerModele/<%=el.Code%>' title="Supprimer cette ligne"><img src="/images/supp.png" alt="suppression"></a>
                <a href='editerModele/<%=el.Code%>' title="Éditer cette ligne"><img src="/images/edit.png" alt="edition"></a></td>
              <td><%=el.Code %></td>
              <td><%=el.Libelle %></td>
              <td><%=el.Marque %></td>
              <td><%=el.Type %></td>
              <td><%=el.Puissance %></td>
              <td><%=el.CodeTarif %></td>
            </tr>
          <%_ }); _%>
        </tbody>
      </table>
      <hr>
  </div>
</div>
<% include ./footer %>